<%= javascript_include_tag "select2.min.js" %>
<%= javascript_include_tag "off-canvas.js" %>
<%= javascript_include_tag "misc.js" %>
<%= javascript_include_tag "jquery-freezetable.js" %>
<!-- endinject -->
<!-- Custom js for this page -->
